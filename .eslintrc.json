{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "root": true,
    "extends": "standard-with-typescript",
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/no-misused-promises": [
            "error",
            {
                "checksVoidReturn": false
            }
            ]
    }
}